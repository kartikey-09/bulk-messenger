{
    "manifest_version": 3,
    "name": "bulk message",
    "version": "1.0",
    "permissions": ["tabs", "activeTab"],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "./src/index.html"
    }
}
